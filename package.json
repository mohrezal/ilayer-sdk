  "author": "iLayer",
  "keywords": [
    "iLayer",
    "rfq",
    "order",
    "sdk"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "viem": "^2.x.x"
  },
  "dependencies": {
    "@layerzerolabs/lz-v2-utilities": "^3.0.120",
    "pusher-js": "^8.4.0"
